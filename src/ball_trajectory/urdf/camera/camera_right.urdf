  name="camera_right">

  <link name="world" />

  <joint name="base_joint" type="fixed">
    <parent link="world"/>
    <child link="body" />
    <origin xyz="0 0 0.010" rpy="0 0 0"/>
  </joint>

  <link name="body">
    <inertial>
      <origin
        xyz="0 0 0.5"
        rpy="0 0 0" />
      <mass
        value="1" />
      <inertia
        ixx="0.003958333"
        ixy="0"
        ixz="0"
        iyy="0.003958333"
        iyz="0"
        izz="0.02" />
    </inertial>
    
    <visual>
      <origin
          xyz="0 0 0.5"
          rpy="0 0 0" />
      <geometry>
        <cylinder length="1" radius="0.05"/>
      </geometry>
      <material
        name="">
        <color
          rgba="0.84706 0.83922 0.83922 1" />
      </material>
    </visual>

    <collision>
      <origin
        xyz="0 0 0.5"
        rpy="0 0 0" />
      <geometry>
        <cylinder length="1" radius="0.05"/>
      </geometry>
    </collision>
    </link>


    <joint name="camera_joint_0" type="fixed">
        <origin xyz="0 0 1" rpy="0 0 -2.355"/>
        <parent link="body"/>
        <child link="camera_link_0"/>
    </joint>


    <link name="camera_link_0">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.1 0.15 0.1" />
            </geometry>
        </visual>

        <collision>
            <origin xyz="0.003 0.065 0.007" rpy="0 0 0"/>
            <geometry>
                <box size="0.012 0.132 0.020"/>
            </geometry>
        </collision>    
    </link>

    <joint name="camera_joint_1" type="fixed">
        <origin xyz="0 0 1" rpy="0 0 -0.785398"/>
        <parent link="body"/>
        <child link="camera_link_1"/>
    </joint>

    <link name="camera_link_1">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.1 0.15 0.1" />
            </geometry>
        </visual>
    </link>

